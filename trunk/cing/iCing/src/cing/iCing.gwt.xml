<module>

      <!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />
        
      <!-- Inherit the default GWT style sheet.  Changed to use explicit files from gwt for instantiations.       
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	-->
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name='com.allen_sauer.gwt.voices.gwt-voices' /> <!-- Inherit sound support                        -->
	<inherits name='com.gwtsolutions.components.Components' /> <!-- Some cool effects; like a shaking dialog box called Message -->
	<inherits name='com.google.gwt.i18n.I18N' />
	<entry-point class="cing.client.iCing" />

   <!-- Specify the servlet that handles file uploads. -->
	<servlet path="/fileupload" class="cing.server.FileUploadServlet" />
</module>